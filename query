CREATE TABLE chat (
	chat_id int PRIMARY KEY AUTO_INCREMENT,
    chat_to varchar(255),
    chat_uid varchar(255),
    message text,
    chat_time timestamp
)

CREATE TABLE bfp_form1 (
	form1_id int PRIMARY KEY AUTO_INCREMENT,
    uid varchar(255),
    fname text,
    mname text,
    lname text,

    gender varchar(255),
    bday text,
    brgy varchar(255),
    
    type text,
    structure text,
    nc_or_rn varchar(255),
    inspection text,
    issuance text,
    form1_status varchar(255),
    form1_date_added timestamp
);

CREATE TABLE bfp_form2 (
    form2_id int PRIMARY KEY AUTO_INCREMENT,
    uid varchar(255),
    fname text,
    mname text,
    lname text,
    gender varchar(255),
    bday text,
    brgy varchar(255),

    type text,
    structure text,
    fsic text,
    with_or_not text,
    reinspection_1 text,
    reinspection_2 text,
    reinspection_3 text,
    closure text,

    form2_status varchar(255),
    form2_date_added timestamp
);

CREATE TABLE bfp_form3 (
    form3_id int PRIMARY KEY AUTO_INCREMENT,
    uid varchar(255),
    fname text,
    mname text,
    lname text,
    gender varchar(255),
    bday text,
    brgy varchar(255),

    type text,
    structure text,
    new_renew text,
    with_or_not text,
    
    reinspection_1 text,
    reinspection_2 text,
    reinspection_3 text,
    closure text,

    form3_status varchar(255),
    form3_date_added timestamp
);

CREATE TABLE bfp_form4 (
    form4_id int PRIMARY KEY AUTO_INCREMENT,
    uid varchar(255),
    fname text,
    mname text,
    lname text,
    gender varchar(255),
    bday text,
    brgy varchar(255),
    fire_code_fees text,
    form4_status varchar(255),
    form4_date_added timestamp
);

CREATE TABLE report(
	report_id int PRIMARY KEY AUTO_INCREMENT,
    details text,
    start_report datetime,
    end_report datetime,
    report_status varchar(255),
    report_submit timestamp
)

CREATE TABLE M_report(
	M_report_id int PRIMARY KEY AUTO_INCREMENT,
    M_details text,
    M_start_report datetime,
    M_end_report datetime,
    M_report_status varchar(255),
    M_report_submit timestamp
)




