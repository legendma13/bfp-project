    newfsicwithprescibed int,
    newfsicnotprescibed int,
    renewfsicwithprescibed int,
    renewfsicnotprescibed int,
    total2 int,
    nonntc int,
    compliantntc int,
    nonNTCV int,
    complaintfsic int,
    nonabatement int,
    totalabatement int,
    total3 int,
    failtocomply int,
    failtopay int,
    overallgov int,
    overallgovinc int,
    overallcompliant int,
    totalreins int,
    form3_date timestamp
)

