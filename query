CREATE TABLE formsubmit (
	form_id int PRIMARY KEY AUTO_INCREMENT,
    uid varchar(255),
    report_ids text,
    form_type text,
    report_name text,
    report_comment text,
    report_status varchar(255),
    submit_date timestamp
)

CREATE TABLE form2 (
	form2_id int PRIMARY KEY AUTO_INCREMENT,
    uid varchar(255),
    brgy text,
    new int,
    renew int,
    nc1 int,
    nc2 int,
    nc3 int,
    nc4 int,
    nc5 int,
    nc6 int,
    nc7 int,
    nc8 int,
    nc9 int,
    nc10 int,
    nc11 int,
    nc12 int,
    nc13 int,
    nc14 int,
    nc15 int,
    nc16 int,
    nonstructure int,
    fsicwith int,
    fsicnot int,
    renewfsicwith int,
    renewfsicnot int,
    issuedntc int,
    issuedntcbusiness int,
    issuedntcv int,
    issuedntcvbusiness int,
    issuedabatement int,
    totalissuedfsic int,
    complyorder int,
    topay int,
    form2_date timestamp
)

CREATE TABLE form3 (
    form3_id int PRIMARY KEY AUTO_INCREMENT,
    uid varchar(255),
    brgy text,
    new int,
    renew int,
    total int,
    nc1 int,
    nc2 int,
    nc3 int,
    nc4 int,
